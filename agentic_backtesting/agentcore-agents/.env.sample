# AWS Bedrock Configuration
# Copy this file to .env and fill in your values

# AWS Region for Bedrock
AWS_REGION=us-east-1

# Default Bedrock Model Configuration (used by base_agent)
BEDROCK_MODEL_ID=us.anthropic.claude-sonnet-4-20250514-v1:0
BEDROCK_TEMPERATURE=0.3

# Strategy Generator Agent Configuration
STRATEGY_GENERATOR_MODEL_ID=us.anthropic.claude-sonnet-4-20250514-v1:0
STRATEGY_GENERATOR_TEMPERATURE=0.3

# Results Summary Agent Configuration
RESULTS_SUMMARY_MODEL_ID=us.amazon.nova-pro-v1:0
RESULTS_SUMMARY_TEMPERATURE=0.3

# AWS Credentials (if not using IAM role)
# AWS_ACCESS_KEY_ID=your_access_key_here
# AWS_SECRET_ACCESS_KEY=your_secret_key_here

# Optional: Model-specific settings
# BEDROCK_MAX_TOKENS=4096
# BEDROCK_TOP_P=0.9
