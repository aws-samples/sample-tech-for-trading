{
  "mcpServers": {
    "awslabs.redshift-mcp-server": {
      "command": "uvx",
      "args": ["awslabs.redshift-mcp-server@latest"],
      "env": {
        "AWS_PROFILE": "factor",
        "AWS_DEFAULT_REGION": "us-east-1",
        "FASTMCP_LOG_LEVEL": "INFO",
        "REDSHIFT_CLUSTER": "trading-redshift-cluster",
        "REDSHIFT_DATABASE": "quantdb"
      },
      "disabled": false,
      "autoApprove": []
    }
  }
}
